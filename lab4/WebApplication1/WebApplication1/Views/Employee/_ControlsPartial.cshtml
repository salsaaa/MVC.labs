@model int?
<div class="btn-group" role="group" aria-label="Basic example">
    <a href="@Url.Action("Details",new { id = Model })" class="btn btn-primary btn-sm">
        <i class="fa fa-eye"></i>

    </a>
    <a href="@Url.Action("Edit",new { id = Model })" class="btn btn-success btn-sm">
        <i class="fa fa-edit"></i>

    </a>
    <button class="btn btn-danger btn-sm" onclick="confirmDelete(@Model)">

        <i class="fa fa-minus-circle"></i>
    </button>
    

</div>